{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "React Boilerplate",
  "main": "index.js",
  "scripts": {
    "start": "xo && node server.js",
    "test": "xo && karma start --single-run",
    "test-ci": "karma start --single-run --reporters coverage,progress,teamcity",
    "testing": "karma start --auto-watch",
    "build": "jspm bundle src/main jspm_packages/build.js"
  },
  "xo": {
    "space": 4,
    "ignores": [
      "jspm_packages/**",
      "node_modules/**",
      "config.js"
    ],
    "envs": [
      "node",
      "browser",
      "mocha"
    ],
    "rules": {
      "react/jsx-indent": [
        2,
        4
      ],
      "no-unused-expressions": 0,
      "react/no-string-refs": 0
    },
    "extends": [
      "xo-react"
    ]
  },
  "keywords": [
    "ui"
  ],
  "author": "React Boilerplate",
  "dependencies": {
    "express": "^4.13.4",
    "http": "0.0.0",
    "node-fetch": "^1.3.3",
    "request-debug": "^0.2.0"
  },
